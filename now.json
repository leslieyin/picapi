{
  "functions": {
    "api/bing.php": {
      "runtime": "vercel-php@0.3.1",
      "excludeFiles": "{test/**}",
      "memory": 256,
      "maxDuration": 5
    }
  },
  "routes": [
    {
      "src": "/(\\w*)\\.(?:jpg|jpeg|png|gif|bmp|webp)",
      "dest": "/api/bing.php?id=$1"
	  "dest": "/api/daohang.php?id=$1"
    }
  ]
}
